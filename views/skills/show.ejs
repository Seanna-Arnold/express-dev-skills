<%- include('../partials/header') %>

<h3><%= skill.skill %></h3>
<h3>Complete: <%= skill.done ? 'Yes' : 'No' %></h3>
<h3>Skills as of <%= time %></h3>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <form action="/skills/<%= s.id %>?_method=DELETE" method="PUT?">
        <button type="submit">X</button>
      </form>
      <a href="/skills/<%= s.id %>"><%= s.skill %></a>
      <%= s.skill %>
        - 
      <%= s.done ? 'done' : 'not done' %>
    </li>
  <% }); %>
</ul>

<a href="/skills/new">Add Skill</a>
<%- include('../partials/footer') %>